        "!pip install gradio\n",
        "import gradio as gr\n",
        "from typing import TypedDict, List\n",
        "from langchain_core.messages import HumanMessage\n",
        "from langchain_core.prompts import ChatPromptTemplate\n",
        "from langchain_groq import ChatGroq\n",
        "import os\n",
        "\n",
        "# Configuration\n",
        "GROQ_API_KEY = os.getenv(\"GROQ_API_KEY\", \"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\")  # Set your actual key here\n",
        "os.environ[\"GROQ_API_KEY\"] = GROQ_API_KEY\n",
        "\n",
        "# State management\n",
        "class TravelState(TypedDict):\n",
        "    city: str\n",
        "    interests: List[str]\n",
        "    stage: str  # 'city' -> 'interests' -> 'itinerary'\n",
        "\n",
        "# Initialize LLM\n",
        "llm = ChatGroq(\n",
        "    temperature=0.7,\n",
        "    model_name=\"llama3-70b-8192\",\n",
        ")\n",
        "\n",
        "# System prompts\n",
        "city_prompt = ChatPromptTemplate.from_messages([\n",
        "    (\"system\", \"Extract just the city name from this message. Reply only with the city name or 'None'.\"),\n",
        "    (\"human\", \"{input}\")\n",
        "])\n",
        "\n",
        "interests_prompt = ChatPromptTemplate.from_messages([\n",
        "    (\"system\", \"Extract travel interests as comma-separated values. Reply only with interests or 'None'.\"),\n",
        "    (\"human\", \"{input}\")\n",
        "])\n",
        "\n",
        "itinerary_prompt = ChatPromptTemplate.from_messages([\n",
        "    (\"system\", \"Create a detailed 1-day itinerary for {city} covering {interests}. Include specific locations, timings, and tips.\"),\n",
        "    (\"human\", \"Please make an itinerary\")\n",
        "])\n",
        "\n",
        "def handle_response(message: str, state: TravelState) -> tuple[str, TravelState]:\n",
        "    if state is None:\n",
        "        state = {'city': '', 'interests': [], 'stage': 'city'}\n",
        "\n",
        "    try:\n",
        "        if state['stage'] == 'city':\n",
        "            city = llm.invoke(city_prompt.format(input=message)).content\n",
        "            if city.lower() != 'none':\n",
        "                state['city'] = city\n",
        "                state['stage'] = 'interests'\n",
        "                return f\"Great! Planning for {city}. What are your interests? (e.g., museums, food)\", state\n",
        "            return \"Please tell me which city you're visiting.\", state\n",
        "\n",
        "        elif state['stage'] == 'interests':\n",
        "            interests = llm.invoke(interests_prompt.format(input=message)).content\n",
        "            if interests.lower() != 'none':\n",
        "                state['interests'] = [i.strip() for i in interests.split(',')]\n",
        "                itinerary = llm.invoke(itinerary_prompt.format(\n",
        "                    city=state['city'],\n",
        "                    interests=', '.join(state['interests'])\n",
        "                )).content\n",
        "                state['stage'] = 'itinerary'\n",
        "                return itinerary, state\n",
        "            return \"Please share your travel interests (e.g., art, history).\", state\n",
        "\n",
        "        return \"How can I help with your trip to \" + state['city'] + \"?\", state\n",
        "\n",
        "    except Exception as e:\n",
        "        return f\"Error: {str(e)}\", state\n",
        "\n",
        "# Gradio interface\n",
        "with gr.Blocks() as demo:\n",
        "    gr.Markdown(\"## ğŸ—ºï¸ Travel Itinerary Planner\")\n",
        "    chatbot = gr.Chatbot(height=400)\n",
        "    message = gr.Textbox(label=\"Your message\", placeholder=\"Type your city or interests...\")\n",
        "    state = gr.State()\n",
        "\n",
        "    def respond(message, chat_history, state):\n",
        "        response, new_state = handle_response(message, state)\n",
        "        chat_history.append((message, response))\n",
        "        return chat_history, new_state\n",
        "\n",
        "    message.submit(respond, [message, chatbot, state], [chatbot, state])\n",
        "    clear = gr.Button(\"Clear\")\n",
        "    clear.click(lambda: ([], {'city': '', 'interests': [], 'stage': 'city'}), None, [chatbot, state])\n",
        "\n",
        "demo.launch(share=True)"
      ],
